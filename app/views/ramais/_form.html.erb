<%= form_for @ramal, :html => { :class => 'form-horizontal' } do |f| %>
  
  <div class="control-group">
    <%= f.label :tipo_protocolo, :class => 'control-label' %>
    <div class="controls">
      <%= f.select :tipo_protocolo, lista_tipo_protocolo %>
    </div>
  </div>

  <div class="control-group">
    <%= f.label :ramal, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :ramal, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :callerid, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :callerid, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :tipo, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :tipo, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :username, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :username, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :secret, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :secret, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :canreinvite, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :canreinvite, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :host, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :host, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :context, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :context, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :dtmfmode, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :dtmfmode, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :call_limit, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :call_limit, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :nat, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :nat, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                ramais_path, :class => 'btn' %>
  </div>
<% end %>
